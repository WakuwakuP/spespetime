  </t>
  <t sync="^14:(6814|6815|6816|6818)" notice="前面安置" notice-d="Sub">
    <v-notice text="ウィング:前面安置" duration="9" />
  </t>

  <!--刻印のトリガーset-->
  <t sync="^1A:[id8]:[mex] gains the effect of 激流の刻印" notice="刻印ついた">
