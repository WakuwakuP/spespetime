      <pre name="overflow" value="true" />
    </expressions>
    <v-notice text="後 AoE処理" duration="30" />
  </t>
  <t sync="^1A:[id8]:[mex] gains the effect of 激動の刻印：(前|後|左|右) from  for 28.00 Seconds." notice="先 AoE処理">
